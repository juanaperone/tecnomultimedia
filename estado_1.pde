
void boton(float x, float y){
  
  pushStyle();
  
  stroke (255);
  fill(0,255,0);
  rect(x,y,300,50);
  
  popStyle();
  
}

void boton2(float x, float y){
  
  pushStyle();
  
  stroke (0);
  fill(0,255,0);
  rect(x,y,150,50);
  
  popStyle();
}

 
 
 


 
 
 
 
 
